{
  "compilerOptions": {
    "module": "system",
    "baseUrl": "src",
    "outFile": "webgl.js",
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "jsx": "react",
    "noUnusedParameters": true,
    "noUnusedLocals": true
  },
  "include": [
    "src/**/*"
  ]
}
